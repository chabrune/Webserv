server {
    root ressources
    listen 8081
    server_name localhost
    index /cyber.html
    allow GET
    autoindex on
    location /tmp/ {
        autoindex on
        root ressources/tmp
        allow GET
        return 301 https://www.winxclubfrance.com/
    }
    return 301 https://www.youtube.com/watch?v=oHg5SJYRHA0
}

server {
    listen 8080
    server_name localhost
    root ressources
    client_max_body_size 3000000
    cgi .py /usr/bin/python3
    index /cyber.html
    autoindex off
    allow GET POST DELETE

    location /assets {
        root ressources/assets
        autoindex on
        allow GET POST PUT HEAD
    }

    location /assets/ali {
        root experiment/expe_ali/site
        allow GET POST
        index index.html
    }
    return 301 https://www.youtube.com/watch?v=oHg5SJYRHA0
}
