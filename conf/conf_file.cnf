server {
    root ressources
    listen 8081
    server_name k0r2p6
    index /cyber.html
    allow GET
    return 301 https://wwww.google.com
}

server {
    listen 8080
    server_name k0r2p6
    root ressources
    client_max_body_size 3000000
    cgi .py ./test/py-cgi
    autoindex on
    allow GET POST DELETE

    location /assets {
        root ressources/assets
        autoindex on
        allow GET POST PUT HEAD
    }
}
